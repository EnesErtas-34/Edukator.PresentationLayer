@model Edukator.EntityLayer.Concrete.Teacher
@{
    ViewData["Title"] = "AddTeacher";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="col-sm-12 col-xl-12">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4">Yeni Eğitmen Girişi</h6>
        <form method="post">
            <div class="mb-3">
                <label class="form-label">Ad Soyad</label>
                <input type="text" class="form-control" asp-for="NameSurname" />
            </div>
            <div class="mb-3">
                <label class="form-label">Unvan</label>
                <input type="text" class="form-control" asp-for="Title" />
            </div>
            <div class="mb-3">
                <label class="form-label">Görsel Url</label>
                <input type="text" class="form-control" asp-for="ImageURL" />
            </div>
            <div class="mb-3">
                <label class="form-label">Facebook</label>
                <input type="text" class="form-control" asp-for="Facebook" />
            </div>
            <div class="mb-3">
                <label class="form-label">Twitter</label>
                <input type="text" class="form-control" asp-for="Twitter" />
            </div>
            <div class="mb-3">
                <label class="form-label">Linkedin</label>
                <input type="text" class="form-control" asp-for="Linkedin" />
            </div>
         

            <button type="submit" class="btn btn-primary">Kaydet</button>
        </form>
    </div>
</div>


