@model Edukator.EntityLayer.Concrete.Skill
@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="col-sm-12 col-xl-12">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4">Yeni Yetenek Girişi</h6>
        <form method="post">
            <div class="mb-3">
                <label class="form-label">Yetenek</label>
                <input type="text" class="form-control" asp-for="Talent" />
            </div>
            <div class="mb-3">
                <label class="form-label">Değer</label>
                <input type="text" class="form-control" asp-for="Value" />
            </div>
            <button type="submit" class="btn btn-primary">Kaydet</button>
        </form>
    </div>
</div>

